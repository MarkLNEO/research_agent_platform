{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:full",
    "dev:client": "vite --host=127.0.0.1 --port=5173",
    "dev:api": "node server/index.js",
    "dev:full": "concurrently -k \"npm run dev:api\" \"npm run dev:client\"",
    "build": "vite build",
    "lint": "eslint .",
    "lint:scope": "eslint src server api scripts --ext .ts,.tsx --report-unused-disable-directives",
    "lint:file": "eslint --ext .ts,.tsx",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit -p tsconfig.app.json",
    "typecheck:diag": "tsc --noEmit -p tsconfig.app.json --diagnostics --extendedDiagnostics --pretty false",
    "test:e2e": "playwright test",
    "test:ui": "playwright test --ui",
    "test:memory": "tsx tests/unit/memory-block.test.ts"
  },
  "dependencies": {
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.57.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jspdf": "^3.0.3",
    "lucide-react": "^0.344.0",
    "openai": "^6.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.3",
    "resend": "^6.1.2",
    "streamdown": "^1.3.0"
  },
  "devDependencies": {
    "@vercel/node": "^3.0.15",
    "@axe-core/playwright": "^4.10.2",
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.55.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "tsx": "^4.19.1"
  }
}
